{
  "plugins": ["plugins/markdown", "plugins/summarize"],
  "recurseDepth": 10,
  "source": {
    "include": ["./extensions/README.md", "./extensions/"],
    "includePattern": "index.js$",
    "excludePattern": "node_modules|build"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "destination": "./docs/api/extensions/",
    "template": "../node_modules/docdash",
    "recurse": true,
    "encoding": "utf8"
  }
}
