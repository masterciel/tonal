{
  "source": {
    "include": ["README.md", "modules/"],
    "includePattern": "index.js$",
    "excludePattern": "node_modules"
  },
  "templates": {
    "default": {
      "outputSourceFiles": false
    },
    "cleverLinks": false,
    "monospaceLinks": true,
    "systemName": "remoteFileExplorer",
    "footer": "tonal",
    "copyright": "2016",
    "navType": "vertical",
    "theme": "spacelab",
    "linenums": true,
    "collapseSymbols": true,
    "inverseNav": false
  },
  "opts": {
    "destination": "./docs/api/",
    "recurse": true,
    "verbose": true,
    "template": "./node_modules/postman-jsdoc-theme"
  },
  "plugins": ["plugins/markdown"]
}
