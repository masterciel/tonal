{
  "name": "tonal",
  "version": "0.30.0",
  "description": "Music tonal kit",
  "main": "index.js",
  "scripts": {
    "build": "npm run disc && npm run dist",
    "test": "vows --spec test/*.js",
    "docs": "documentation -f html -o docs support/docgen.js && documentation -f md support/docgen.js > API.md",
    "dist": "browserify index.js | uglifyjs > dist/tonal.min.js&& ls -hall dist/",
    "disc": "browserify --full-paths index.js | uglifyjs | discify > dist/disc.html",
    "lint": "jshint .",
    "update": "rm -rf node_modules/tonal.* && npm i && npm dedupe"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danigb/tonal.git"
  },
  "keywords": [
    "music",
    "theory",
    "note",
    "notes",
    "transpose",
    "midi",
    "interval",
    "chords",
    "chord",
    "scale",
    "scales",
    "key",
    "tonal"
  ],
  "author": "danigb",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danigb/tonal/issues"
  },
  "homepage": "https://github.com/danigb/tonal#readme",
  "devDependencies": {
    "lerna": "^1.0.4",
    "vows": "^0.8.x"
  },
  "dependencies": {
    "tonal.chord": "0.0.x",
    "tonal.dictionary": "0.0.x",
    "tonal.distance": "0.0.x",
    "tonal.gamut": "0.0.x",
    "tonal.key": "0.0.x",
    "tonal.notation": "0.0.x",
    "tonal.note": "0.0.1",
    "tonal.pitch": "0.0.x",
    "tonal.scale": "0.0.2",
    "tonal.set": "0.0.x",
    "tonal.transpose": "0.0.x"
  }
}
