{
  "name": "tonal",
  "version": "0.40.22",
  "description": "A functional music theory library",
  "main": "dist/tonal.js",
  "jsnext:main": "lib/tonal.js",
  "scripts": {
    "bundle": "npm test && npm run dist && npm run docco && npm run api && ls -hall dist/",
    "standard": "standard lib/tonal.js",
    "packages-install": "rollup -c && for d in ./packages/*/ ; do (cd \"$d\" && npm i ../..); done",
    "packages-build": "for d in ./packages/*/ ; do (echo \"$d\" && cd \"$d\" && rollup -c); done",
    "packages-api": "for d in ./packages/*/ ; do (echo \"$d\" && cd \"$d\" && jsdoc2md --global-index-format none lib/index.js > API.md); done",
    "packages-test": "_mocha `scripts/get-test-directories.sh`",
    "test": "npm run rollup && mocha",
    "test-ci": "npm run packages-build && npm run packages-test",
    "docco": "docco -l linear lib/tonal.js",
    "docs": "npm run docco && open docs/tonal.html",
    "api": "jsdoc2md lib/tonal.js > docs/API.md",
    "rollup": "rollup lib/tonal.js -o dist/tonal.js -f cjs",
    "dist": "rollup -c && uglifyjs dist/tonal.js > dist/tonal.min.js",
    "disc": "cat dist/tonal.min.js | discify > dist/disc.html",
    "coverage": "istanbul cover _mocha",
    "cover": "npm run coverage && open coverage/lcov-report/index.html"
  },
  "homepage": "https://github.com/danigb/tonal#readme",
  "repository": "https://github.com/danigb/tonal/tree/master/packages/tonal",
  "bugs": {
    "url": "https://github.com/danigb/tonal/issues"
  },
  "keywords": [
    "music",
    "tonal",
    "theory",
    "pitch",
    "interval",
    "tonal"
  ],
  "author": "danigb",
  "license": "MIT",
  "dependencies": {
    "interval-notation": "^0.1.0",
    "note-parser": "^1.0.0"
  },
  "devDependencies": {
    "babel-preset-es2015-rollup": "^1.1.1",
    "lerna": "^1.1.2",
    "markdown-code-blocks": "0.0.1",
    "mocha": "^2.4.5",
    "rollup": "^0.25.8",
    "rollup-plugin-babel": "^2.4.0",
    "rollup-plugin-json": "^2.0.0",
    "standard": "^6.0.8"
  }
}
