{
  "name": "tonal-tools",
  "version": "0.1.0",
  "private": "true",
  "description": "Music theory for javascript",
  "scripts": {
    "test": "vows --spec test/*.js test/**/*.js",
    "docs": "node ./utils/generateDocs.js",
    "build": "node ./utils/generateFacades.js",
    "dist": "browserify ./lib/index.js > dist/tonal.js && browserify ./lib/index.js | uglifyjs -c > dist/tonal.min.js",
    "lint": "jshint .",
    "validate": "npm ls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danigb/tonal.git"
  },
  "keywords": [
    "music",
    "music theory",
    "note",
    "notes",
    "transpose",
    "midi",
    "interval",
    "chords",
    "chord",
    "scale",
    "scales"
  ],
  "author": "danigb",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danigb/tonal/issues"
  },
  "homepage": "https://github.com/danigb/tonal#readme",
  "devDependencies": {
    "benchmark": "^1.0.0",
    "docme": "^0.3.0",
    "dox": "^0.8.0",
    "lodash": "^3.10.1",
    "precommit-hook": "^3.0.0",
    "teoria": "^2.0.1",
    "uglifyify": "^3.0.1",
    "vows": "^0.8.1"
  },
  "pre-commit": [
  ]
}
