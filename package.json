{
  "name": "tonal",
  "version": "2.0.0",
  "description": "A functional music theory library",
  "main": "dist/tonal.js",
  "jsnext:main": "dist/jsnext/tonal.js",
  "scripts": {
    "publish": "npm test && npm run coverage && npm run docs && npm run api && npm run dist && npm run standard",
    "standard": "npm run build && standard dist/jsnext/tonal.js",
    "test": "npm run build && mocha",
    "docco": "docco -l linear lib/tonal.md",
    "docs": "npm run docco && open docs/tonal.html",
    "api": "npm run build && jsdoc2md dist/tonal.js > docs/API.md",
    "rollup": "rollup dist/jsnext/tonal.js -o dist/tonal.js -f cjs",
    "source": "cat lib/tonal.md | markdown-code-blocks -t js > dist/jsnext/tonal.js",
    "build": "npm run source && npm run rollup",
    "dist": "rollup -c && uglifyjs dist/tonal.js > dist/tonal.min.js",
    "coverage": "istanbul cover _mocha",
    "cover": "npm run coverage && open coverage/lcov-report/index.html"
  },
  "homepage": "https://github.com/danigb/tonal#readme",
  "repository": "https://github.com/danigb/tonal/tree/master/packages/tonal",
  "bugs": {
    "url": "https://github.com/danigb/tonal/issues"
  },
  "keywords": [
    "music",
    "tonal",
    "theory",
    "pitch",
    "interval",
    "tonal"
  ],
  "author": "danigb",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "babel-preset-es2015-rollup": "^1.1.1",
    "markdown-code-blocks": "0.0.1",
    "rollup": "^0.25.8",
    "rollup-plugin-babel": "^2.4.0",
    "rollup-plugin-json": "^2.0.0",
    "standard": "^6.0.8"
  }
}
