{
  "name": "tonal-intervals",
  "version": "0.50.2",
  "description": "Music intervals manipulation",
  "main": "dist/index.js",
  "jsnext:main": "lib/index.js",
  "scripts": {
    "bundle": "npm test && npm run dist && npm run docco && npm run api && ls -hall dist/",
    "standard": "standard lib/index.js",
    "test": "npm run rollup && mocha",
    "test-ci": "rollup -c && mocha",
    "docco": "docco -l linear lib/index.js",
    "docs": "npm run docco && open docs/index.html",
    "api": "jsdoc2md lib/index.js > docs/API.md",
    "rollup": "rollup lib/index.js -o dist/index.js -f cjs",
    "dist": "rollup -c && uglifyjs dist/index.js > dist/tonal.min.js",
    "coverage": "istanbul cover _mocha",
    "cover": "npm run coverage && open coverage/lcov-report/index.html"
  },
  "homepage": "https://github.com/danigb/tonal#readme",
  "repository": "https://github.com/danigb/tonal/tree/master/modules/tonal-intervals",
  "bugs": {
    "url": "https://github.com/danigb/tonal/issues"
  },
  "keywords": [
    "music",
    "interval",
    "tonal"
  ],
  "author": "danigb",
  "license": "MIT",
  "dependencies": {
    "tonal-pitches": "^0.50.0",
    "tonal-collections": "^0.50.0"
  },
  "devDependencies": {}
}
