{
  "name": "tonal-ranges",
  "version": "0.50.4",
  "description": "Create ranges of notes",
  "main": "dist/index.js",
  "jsnext:main": "lib/index.js",
  "scripts": {
    "bundle": "npm test && npm run dist && npm run api",
    "standard": "standard lib/index.js",
    "test": "npm run rollup && mocha",
    "api": "jsdoc2md lib/index.js > API.md",
    "rollup": "rollup lib/index.js -o dist/index.js -f cjs",
    "dist": "rollup -c && browserify dist/index.js | uglifyjs > dist/tonal-ranges.min.js && ls -hall dist/",
    "coverage": "istanbul cover _mocha",
    "cover": "npm run coverage && open coverage/lcov-report/index.html"
  },
  "homepage": "https://github.com/danigb/tonal/tree/master/packages/tonal-ranges#readme",
  "repository": "https://github.com/danigb/tonal/tree/master/packages/tonal-ranges",
  "bugs": {
    "url": "https://github.com/danigb/tonal/issues"
  },
  "keywords": [
    "music",
    "theory",
    "note",
    "ranges",
    "tonal"
  ],
  "author": "danigb",
  "license": "MIT",
  "dependencies": {
    "tonal-pitches": "^0.50.4",
    "tonal-notes": "^0.50.4",
    "tonal-collections": "^0.50.4",
    "tonal-distances": "^0.50.4",
    "tonal-midi": "^0.50.4"
  }
}
