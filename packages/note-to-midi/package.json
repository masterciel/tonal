{
  "name": "note-to-midi",
  "version": "0.0.4",
  "description": "Note name to midi conversion",
  "main": "index.js",
  "scripts": {
    "build": "npm test && npm run docs && npm run coverage && npm run disc && npm run dist",
    "test": "mocha",
    "docs": "documentation -f md > API.md",
    "dist": "browserify index.js | uglifyjs > dist/note-to-midi.min.js&& ls -hall dist/",
    "disc": "browserify --full-paths index.js | uglifyjs | discify > dist/disc.html",
    "coverage": "istanbul cover _mocha",
    "cover": "npm run coverage && open ./coverage/lcov-report/index.html",
    "climate": "npm run coverage && npm run codeclimate",
    "codeclimate": "CODECLIMATE_REPO_TOKEN= codeclimate-test-reporter < coverage/lcov.info"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danigb/note-to-midi.git"
  },
  "keywords": [
    "music",
    "note",
    "midi",
    "number",
    "tonal"
  ],
  "author": "danigb",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danigb/tonal/issues"
  },
  "homepage": "https://github.com/danigb/tonal#readme",
  "devDependencies": {
  },
  "dependencies": {
    "tonal.notation": "0.0.x"
  }
}
