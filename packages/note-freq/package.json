{
  "name": "note-freq",
  "version": "1.0.1",
  "description": "Convert from notes to frequencies",
  "main": "index.js",
  "scripts": {
    "build": "npm test && npm run docs && npm run coverage && npm run disc && npm run dist",
    "test": "mocha",
    "docs": "documentation -f md > API.md",
    "dist": "mkdir -p tmp/ && browserify index.js | uglifyjs > tmp/note-freq.min.js&& ls -hall tmp/",
    "disc": "mkdir -p tmp/ && browserify --full-paths index.js | uglifyjs | discify > tmp/disc.html",
    "coverage": "istanbul cover _mocha",
    "cover": "npm run coverage && open ./coverage/lcov-report/index.html"
  },
  "homepage": "https://github.com/danigb/tonal/tree/master/packages/note-freq#readme",
  "repository": "https://github.com/danigb/tonal/tree/master/packages/note-freq",
  "bugs": {
    "url": "https://github.com/danigb/tonal/issues"
  },
  "keywords": [
    "music",
    "theory",
    "note",
    "frequency",
    "tonal"
  ],
  "author": "danigb",
  "license": "MIT",
  "dependencies": {
    "midi-freq": "^1.0.0",
    "note-midi": "^1.0.2"
  }
}
