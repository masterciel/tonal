{
  "name": "tonal-chords",
  "version": "0.50.4",
  "description": "Create and manipulate music chords",
  "main": "dist/index.js",
  "scripts": {
    "bundle": "npm test && npm run dist && npm run docco && npm run api",
    "standard": "standard lib/index.js",
    "test": "npm run rollup && mocha",
    "test-ci": "rollup -c && mocha",
    "docco": "docco -l linear lib/index.js",
    "docs": "npm run docco && open docs/index.html",
    "api": "jsdoc2md lib/index.js > API.md",
    "rollup": "cp lib/chords.json dist/chords.json && rollup lib/index.js -o dist/index.js -f cjs",
    "dist": "rollup -c && browserify dist/index.js | uglifyjs > dist/tonal-chords.min.js && ls -hall dist/",
    "coverage": "istanbul cover _mocha",
    "cover": "npm run coverage && open coverage/lcov-report/index.html"
  },
  "homepage": "https://github.com/danigb/tonal/tree/master/packages/tonal-chords#readme",
  "repository": "https://github.com/danigb/tonal/tree/master/packages/tonal-chords",
  "bugs": {
    "url": "https://github.com/danigb/tonal/issues"
  },
  "keywords": [
    "music",
    "chord",
    "tonal"
  ],
  "author": "danigb",
  "license": "MIT",
  "dependencies": {
    "note-parser": "^1.0.0",
    "tonal-pitches": "^0.50.4",
    "tonal-array": "^0.50.4"
  }
}
