{
  "name": "interval-simplify",
  "version": "1.0.1",
  "description": "Simplify musical intervals",
  "main": "index.js",
  "scripts": {
    "build": "npm test && npm run docs && npm run coverage && npm run disc && npm run dist",
    "test": "mocha",
    "api": "documentation -f md > API.md",
    "dist-no": "mkdir -p tmp/ && browserify index.js | uglifyjs > tmp/interval-simplify.min.js&& ls -hall tmp/",
    "disc": "mkdir -p tmp/ && browserify --full-paths index.js | uglifyjs | discify > tmp/disc.html",
    "coverage": "istanbul cover _mocha",
    "cover": "npm run coverage && open ./coverage/lcov-report/index.html"
  },
  "homepage": "https://github.com/danigb/tonal/tree/master/packages/interval-simplify#readme",
  "repository": "https://github.com/danigb/tonal/tree/master/packages/interval-simplify",
  "bugs": {
    "url": "https://github.com/danigb/tonal/issues"
  },
  "keywords": [
    "music",
    "theory",
    "simplify",
    "interval",
    "tonal"
  ],
  "author": "danigb",
  "license": "MIT",
  "dependencies": {
    "music-notation": "^1.0.1"
  }
}
